using System;
using System.Collections.Generic;
using System.Collections;
using UnityEngine;
namespace BattleFramework.Data{
    [System.Serializable]
    public class D:\SLG\Assets\Resources\Configs\soldierProperty {
        public static string csvFilePath = "sets\Resources\Configs\soldierProperty";
        public List<D:\SLG\Assets\Resources\Configs\soldierProperty> LoadDatas(){
            CSVFile csvFile = new CSVFile();
            csvFile.Open (csvFilePath);
            List<D:\SLG\Assets\Resources\Configs\soldierProperty> dataList = new List<D:\SLG\Assets\Resources\Configs\soldierProperty>();
            for(int i = 0;i < csvFile.mapData.Count;i ++){
                D:\SLG\Assets\Resources\Configs\soldierProperty data = new D:\SLG\Assets\Resources\Configs\soldierProperty();
                dataList.Add(data);
            }
            return dataList;
        }
    }
}
