using System;
using System.Collections.Generic;
using System.Collections;
using UnityEngine;
namespace BattleFramework.Data{
    [System.Serializable]
    public class D:\SLG\Assets\Resources\Configs\castleBuildDesign {
        public static string csvFilePath = "sets\Resources\Configs\castleBuildDesign";
        public List<D:\SLG\Assets\Resources\Configs\castleBuildDesign> LoadDatas(){
            CSVFile csvFile = new CSVFile();
            csvFile.Open (csvFilePath);
            List<D:\SLG\Assets\Resources\Configs\castleBuildDesign> dataList = new List<D:\SLG\Assets\Resources\Configs\castleBuildDesign>();
            for(int i = 0;i < csvFile.mapData.Count;i ++){
                D:\SLG\Assets\Resources\Configs\castleBuildDesign data = new D:\SLG\Assets\Resources\Configs\castleBuildDesign();
                dataList.Add(data);
            }
            return dataList;
        }
    }
}
